;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Card Game Framework"
config/version="0.1"
run/main_scene="uid://csy5ulej80exm"
config/features=PackedStringArray("4.4", "Mobile")
config/icon="res://Img/icon.svg"

[autoload]

Network="*res://Scripts/NetworkController.gd"
GameManager="*res://Scripts/GameManager.gd"
SceneManager="*res://Scripts/Utils/SceneManager.gd"

[display]

